	<content><![CDATA[
	void graphfun(int n,int e){
  	int x,y;
	cin>>n>>e;
	vector<int> v[n+1];

	for (int i = 0; i < e; ++i)
	{
		cin>>x>>y;
		v[x].push_back(y);
    		v[y].push_back(x);
	}
}
]]></content>
	<!-- Optional: Set a tabTrigger to define how to trigger the snippet -->
	<tabTrigger>graphfun</tabTrigger>
